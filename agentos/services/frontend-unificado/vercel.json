{
  "version": 2,
  "builds": [
    {
      "src": "frontend/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/frontend/dist",
        "buildCommand": "cd frontend/frontend && npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://agentos-api.railway.app"
  }
}